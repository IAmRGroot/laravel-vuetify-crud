{
    "name": "iamrgroot/laravel-vuetify-crud",
    "description": "Easily create Vuetify CRUD pages with Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Groot",
            "email": "github@rgroot.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/routing": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "composer/composer": "^2.0",
        "doctrine/dbal": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12.84"
    },
    "autoload": {
        "psr-4": {
            "IAmRGroot\\LaravelVuetifyCRUD\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IAmRGroot\\VuetifyCRUD\\VuetifyCRUDServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "php ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --verbose --config=./php_cs.dist --path-mode=intersection . --ansi"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse"
        ]
    }
}
